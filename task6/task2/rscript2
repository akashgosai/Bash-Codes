library(ggplot2)
t=read.csv("callgraph.csv",header=FALSE)
a=data.frame(t$V1)
b=data.frame(t$V2)
c=data.frame(t$V3)
d=data.frame(t$V4)
ac=data.frame(a,c)
f1=data.frame(a=unlist(ac,use.names=FALSE))
bd=data.frame(b,d)
f2=data.frame(a=unlist(bd,use.names=FALSE))
merged=data.frame(f1,f2)
final=unique(merged)
ggplot(final,aes(x=a,y=a.1)) + geom_col(aes(fill=final$a))

